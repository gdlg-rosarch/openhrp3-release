  <rosdep name="eigen" />
  <rosdep name="atlas" />
  <rosdep name="f2c" />
  <rosdep name="boost" />
  <rosdep name="doxygen" />
  <rosdep name="libjpeg" />
  <rosdep name="libpng12-dev" />
  <rosdep name="subversion" />

  <rosdep name="rostest" />
  <rosdep name="mk" />

  <!-- collada -->
  <rosdep name="collada-dom" />

</package>


